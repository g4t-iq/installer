{
    "name": "harmony-framework/installer",
    "description": "Laravel application installer.",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hussein Alaa",
            "email": "hussein@harmony-framework.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/filesystem": "^10.20",
        "illuminate/support": "^10.20",
        "laravel/prompts": "^0.1",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.3"
    },
    "bin": [
        "bin/harmony"
    ],
    "autoload": {
        "psr-4": {
            "Harmony\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harmony\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
